namespace ConsoleApplication6
{
class Program
    {
static void Main(string[] args)
        {
int i;
int n;
Console.WriteLine("Enter a Number:- ");
            n = Convert.ToInt32(Console.ReadLine());
for (i = 1; i <= n; i++)
            {
Console.WriteLine("Teach One,EachOne,Tree One");
            }
Console.ReadLine();

        }
    }
}
