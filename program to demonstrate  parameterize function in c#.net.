using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ConsoleApplication14
{
    public class Program
    {
        int x = 0;
        void print(int x, int y)
        {
            x = x + y;
            Console.WriteLine(x);
        }
        public static void Main()
        {

            Program p = new Program();
            p.print(10, 10);
            Console.ReadLine();
        }
    }
}
